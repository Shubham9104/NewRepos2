@model IEnumerable<GroceryManagement.web.Models.Category>

@{

}
<h1>Food Categories</h1>
<div class="row">
    @foreach (var item in Model)
    {


        <div class="card col-3" style="width: 18rem;">
            <img class="card-img-top" src="..." alt="Card image cap">
            <div class="card-body">
                <h5 class="card-title">@Html.DisplayFor(modelItem => item.Categories)</h5>
                @* <p class="card-text">@Html.DisplayFor(modelItem => item.IcId)</p>*@
                <a class="btn btn-primary" asp-action="Inside" asp-route-id="@item.IcId">Go somewhere</a>
            </div>
        </div>
    }

</div>

